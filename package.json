{
    "name": "nut",
    "private": true,
    "description": "Network UPS tools (NUT) menu bar app",
    "version": "1.0.0",
    "license": "MIT",
    "main": "build/main.js",
    "author": "Vitaliy Stoyanov",
    "scripts": {
        "start": "npm run build && electron build/main.js --enable-logging",
        "start:dev": "cross-env START_MAIN=1 npm run start:dev:renderer",
        "start:dev:main": "webpack --config config/webpack/webpack.main.config.js && electron build/main.js",
        "start:dev:renderer": "webpack-dev-server --config config/webpack/webpack.renderer.dev.config.js",
        "build": "npm run build:main && npm run build:renderer",
        "build:main": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.main.prod.config.js",
        "build:renderer": "cross-env NODE_ENV=production webpack --config config/webpack/webpack.renderer.prod.config.js",
        "test": "echo \"No tests yet.\" && exit 0",
        "clean": "rimraf build && rimraf dist",
        "format": "prettier --write 'src/**/*.{ts,tsx}'",
        "lint": "eslint --fix 'src/**/*.{ts,tsx}'",
        "predist": "npm run build",
        "dist": "electron-builder -m -c.mac.identity=null",
        "pack": "electron-builder --dir -c.mac.identity=null",
        "pack:universal": "electron-builder --dir --universal -c.mac.identity=null",
        "postinstall": "electron-builder install-app-deps",
        "start:electron": "electron . --enable-logging"
    },
    "dependencies": {
        "@primer/components": "23.2.1",
        "auto-launch": "5.0.5",
        "axios": "^1.5.1",
        "contribution": "4.1.1",
        "electron-store": "7.0.3",
        "electron-unhandled": "^4.0.1",
        "formik": "2.2.9",
        "moment": "^2.29.4",
        "node-schedule": "^2.1.1",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-helmet": "6.1.0",
        "styled-components": "5.3.3",
        "sudo-prompt": "^9.2.1"
    },
    "devDependencies": {
        "@babel/core": "7.16.7",
        "@babel/plugin-proposal-class-properties": "7.16.7",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
        "@babel/plugin-proposal-optional-chaining": "^7.21.0",
        "@babel/polyfill": "7.12.1",
        "@babel/preset-env": "7.16.7",
        "@babel/preset-react": "7.16.7",
        "@babel/preset-typescript": "7.16.7",
        "@commitlint/cli": "12.1.4",
        "@commitlint/config-conventional": "12.1.4",
        "@hot-loader/react-dom": "17.0.1",
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/commit-analyzer": "8.0.1",
        "@semantic-release/git": "9.0.1",
        "@semantic-release/github": "7.2.3",
        "@semantic-release/npm": "7.1.3",
        "@semantic-release/release-notes-generator": "9.0.3",
        "@types/auto-launch": "5.0.2",
        "@types/cron": "1.7.3",
        "@types/jest": "^26.0.24",
        "@types/moment": "^2.13.0",
        "@types/node": "^18.14.6",
        "@types/node-schedule": "^2.1.1",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "@types/react-helmet": "6.1.5",
        "@types/styled-components": "5.1.19",
        "@types/webpack-env": "1.16.3",
        "@typescript-eslint/eslint-plugin": "4.33.0",
        "@typescript-eslint/parser": "4.33.0",
        "babel-loader": "8.2.5",
        "commitizen": "4.3.0",
        "commitlint": "12.1.4",
        "copy-webpack-plugin": "5.1.2",
        "cross-env": "7.0.3",
        "cz-conventional-changelog": "3.3.0",
        "electron": "11.0.0",
        "electron-builder": "24.6.4",
        "eslint": "7.32.0",
        "eslint-config-prettier": "6.15.0",
        "eslint-plugin-prettier": "3.4.1",
        "eslint-plugin-react": "7.28.0",
        "file-loader": "6.2.0",
        "fork-ts-checker-webpack-plugin": "4.1.6",
        "html-webpack-plugin": "4.5.2",
        "husky": "4.3.8",
        "lint-staged": "10.5.4",
        "prettier": "2.5.1",
        "react-hot-loader": "4.13.0",
        "rimraf": "3.0.2",
        "semantic-release": "17.4.7",
        "source-map-loader": "0.2.4",
        "tsconfig-paths-webpack-plugin": "3.5.2",
        "typescript": "^4.5.4",
        "webpack": "4.47.0",
        "webpack-cli": "3.3.12",
        "webpack-dev-server": "3.11.3",
        "webpack-merge": "4.2.2"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
